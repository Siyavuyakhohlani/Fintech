{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "target": "ESNext",
    "lib": ["ESNext"],
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": false,
    "useDefineForClassFields": true,
    "resolveJsonModule": true
  },
  "include": [
    "vite.config.ts"
  ]
}
